{
    "name": "trjili/silex-goutte-service-provider",
    "description": "Goutte service provider for Silex",
    "type": "library",
    "license": "MIT",
    "keywords": [
      "silex",
      "goutte",
      "scraper"
    ],
    "authors": [
        {
            "name": "Tarek Rjili",
            "email": "tarek.rjili@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
      "php": ">=5.4.0",
      "silex/silex" : "~1.1",
      "fabpot/goutte": "~2.0"
    },
    "require-dev": {
      "phpunit/phpunit" : "@stable"
    },
    "autoload": {
      "psr-4": {  "Trjili\\Silex\\": "src/" }
    },
    "autoload-dev": {
      "psr-4": {
        "Trjili\\Silex\\": "tests/"
      }
    }
}
